{
  "containerDefinitions": [
    {
      "name": "tis-common-upload",
      "image": "430723991443.dkr.ecr.eu-west-2.amazonaws.com/tis-common-upload:1",
      "portMappings": [
        {
          "containerPort": 8089
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "tis-common-upload"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-2"
        },
        {
          "name": "SENTRY_ENVIRONMENT",
          "value": "stage"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-preprod-tis-common-upload",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "awslogs-tis-common-upload"
        }
      },
      "secrets": [
        {
          "name": "AWS_ACCESS_KEY",
          "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-common-upload-access-key"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-common-upload-secret-key"
        },
        {
          "name": "SENTRY_DSN",
          "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-common-upload-sentry-dsn"
        }
      ]
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "tis-common-upload",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
